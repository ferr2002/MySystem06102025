{
  "name": "source",
  "version": "1.0.2",
  "main": "./main/main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "electron-builder",
    "dev": "nodemon --watch main --watch preload --exec \"electron .\""
  },
  "build": {
    "appId": "sistema.id",
    "productName": "Sistema Farmacia",
    "asarUnpack": [
    "assets/loja.db"
    ],
    "win": {
      "target": "msi",
      "icon": "build/icon.ico"
    },
    "mac": {
      "icon": "build/icon.icns"
    },
    "linux": {
      "icon": "build/icon.png"
    },
    "extraResources": [
      {
        "from": "./assets/",
        "to": "assets"
      }
    ]
  },
  "author": "I",
  "license": "ISC",
  "description": "Sistema Farmacia",
  "devDependencies": {
    "electron": "^37.2.1",
    "electron-builder": "^26.0.12",
    "electron-reloader": "^1.2.3",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "sqlite3": "^5.1.7"
  }
}
